<%- console.log(JSON.stringify(req.session)) %>
<table border="1px">
    <thead>
    <tr>
        <th>ID</th>
        <th>Username</th>
        <th>Senha</th>
    </tr>
    </thead>
    <tbody>
        <% let i; for(i=0; i<dados.length; i++) { %>
            <% if (dados [i].username==req.session.username) {%>
                <tr style="background-color: rgb(73, 10, 156); color: white;">
                    <%} else {%>
                <tr>
                    <% }%>
            
                <td><%= dados[i].id %></td>
                <td><%= dados[i].username %></td>
                <td><%= dados[i].password %></td>
            </tr>
            <% } %>
    </tbody>
    <tfoot>
        <tr><th colspan="3">
            <%- `Qtd de registros ${i}` %>
        </th></tr>
    </tfoot>
</table>