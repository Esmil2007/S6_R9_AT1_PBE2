<h1><%= titulo %></h1>
<% criticas.forEach(critica => { %>
    <div style="border: 1px solid #ccc; margin: 10px; padding: 10px;">
        <h2><%= critica.titulo %> - <%= critica.destino %></h2>
        <p><%= critica.conteudo %></p>
        <strong>Nota:</strong> <%= critica.nota %>/5<br>
        <small>Data: <%= critica.data_criacao %></small>
    </div>
<% }); %>